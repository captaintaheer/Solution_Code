{
  "engines": {
    "composer": "^0.20.0"
  },
  "name": "medical-network",
  "version": "1.0.6",
  "description": "medical blockchain",
  "scripts": {
    "prepublish": "mkdirp ./dist && composer archive create --sourceType dir --sourceName . -a ./dist/medical-network.bna",
    "pretest": "npm run lint",
    "lint": "eslint .",
    "test": "nyc mocha -t 0 test/*.js && cucumber-js"
  },
  "keywords": [
    "composer",
    "composer-network"
  ],
  "author": "Jack",
  "email": "jack@example.com",
  "license": "Apache-2.0",
  "devDependencies": {
    "composer-admin": "^0.20.0",
    "composer-cli": "^0.20.0",
    "composer-client": "^0.20.0",
    "composer-common": "^0.20.0",
    "composer-connector-embedded": "^0.20.0",
    "composer-cucumber-steps": "^0.20.0",
    "chai": "latest",
    "chai-as-promised": "latest",
    "cucumber": "^2.2.0",
    "eslint": "latest",
    "nyc": "latest",
    "mkdirp": "latest",
    "mocha": "latest",
    "jsencrypt": "latest",
    "sjcl": "latest",
    "crypto-js": "latest",
    "navigator": "latest",
    "window": "latest",
    "node-jsencrypt": "latest"
  },
  "dependencies": {
    "angular-modal-service": "^0.13.0"
  },
  "eslintConfig": {
    "rules": {
      "require-jsdoc": "off",
      "no-trailing-spaces": "off",
      "valid-jsdoc": "off",
      "quotes": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "indent": "off",
      "require-atomic-updates": "off"
    },
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true
    }
  }
}
